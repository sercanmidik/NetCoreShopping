@model Task<IEnumerable<ResultCategoryCountListDto>>

    @foreach (var item in await Model)
{
    <a data-toggle="collapse" href="#fruitsVegetable" aria-expanded="false" aria-controls="fruitsVegetable">
        <span class="lnr lnr-arrow-right"></span>@item.CategoryName<span class="number">(@item.CategoryCount)</span>
    </a>
}
